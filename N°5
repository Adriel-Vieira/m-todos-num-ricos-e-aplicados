      ],
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Definição da função original\n",
        "def f(x):\n",
        "    return x**2 - 2*(np.cos(x)**2 - 2*(np.sin(x)**3))\n",
        "\n",
        "# Definição da derivada da função\n",
        "def df(x):\n",
        "    return 2*x + 2*(2*np.cos(x)*np.sin(x) + 6*np.sin(x)**2*np.cos(x))\n",
        "\n",
        "# Geração de valores para o gráfico da derivada\n",
        "x_values = np.linspace(-3, 3, 400)\n",
        "y_values = df(x_values)\n",
        "\n",
        "# Plotando o gráfico da derivada\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.plot(x_values, y_values, label=r\"$f'(x) = 2x + 2(2\\cos(x)\\sin(x) + 6\\sin^2(x)\\cos(x))$\")\n",
        "plt.axhline(0, color='black', lw=0.5, ls='--')\n",
        "plt.axvline(0, color='black', lw=0.5, ls='--')\n",
        "plt.title(\"Gráfico da Derivada da Função\")\n",
        "plt.xlabel(\"x\")\n",
        "plt.ylabel(\"f'(x)\")\n",
        "plt.grid()\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "# Cálculo da derivada no ponto x = 5\n",
        "x_point = 5\n",
        "derivative_at_5 = df(x_point)\n",
        "\n",
        "# Exibindo resultado da derivada\n",
        "print(f\"A derivada da função em x = {x_point} é aproximadamente: {derivative_at_5:.6f}\")\n",
        "\n",
        "# Tabela de iterações (exemplo simples usando diferenças finitas)\n",
        "h_values = [0.1, 0.01, 0.001]\n",
        "iterations = []\n",
        "\n",
        "for h in h_values:\n",
        "    numerical_derivative = (f(x_point + h) - f(x_point - h)) / (2 * h)\n",
        "    iterations.append((h, numerical_derivative))\n",
        "\n",
        "# Exibindo a tabela de iterações\n",
        "print(\"\\nTabela de iterações para cálculo da derivada numérica:\")\n",
        "print(f\"{'h':<10} {'Derivada Numérica':<20}\")\n",
        "for h, num_derivative in iterations:\n",
        "    print(f\"{h:<10} {num_derivative:<20.6f}\")\n"
      ]
    }
  ]
