      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Definindo a função f(x) = (cos(x))^2 - cos(x)\n",
        "def f(x):\n",
        "    return (np.cos(x))**2 - np.cos(x)\n",
        "\n",
        "# Derivada da função para o método de Newton-Raphson\n",
        "def f_prime(x):\n",
        "    return -2 * np.cos(x) * np.sin(x) + np.sin(x)\n",
        "\n",
        "# Implementação do método da bisseção\n",
        "def bissecao(a, b, tol, max_iter):\n",
        "    iteracoes = []\n",
        "    for i in range(max_iter):\n",
        "        c = (a + b) / 2\n",
        "        iteracoes.append((i, a, b, c, f(c)))\n",
        "        if f(c) == 0 or (b - a) / 2 < tol:\n",
        "            return c, iteracoes\n",
        "        if f(c) * f(a) < 0:\n",
        "            b = c\n",
        "        else:\n",
        "            a = c\n",
        "    return c, iteracoes\n",
        "\n",
        "# Implementação do método de Newton-Raphson\n",
        "def newton_raphson(x0, tol, max_iter):\n",
        "    iteracoes = []\n",
        "    for i in range(max_iter):\n",
        "        x1 = x0 - f(x0) / f_prime(x0)\n",
        "        iteracoes.append((i, x0, f(x0)))\n",
        "        if abs(x1 - x0) < tol:\n",
        "            return x1, iteracoes\n",
        "        x0 = x1\n",
        "    return x0, iteracoes\n",
        "\n",
        "# Gerando o gráfico da função no intervalo [-3, 0]\n",
        "a, b = -3, 0\n",
        "tol = 1e-5\n",
        "max_iter = 100\n",
        "x0 = -1.5\n",
        "\n",
        "# Aplicando os métodos para obter as raízes\n",
        "raiz_bissecao, it_bissecao = bissecao(a, b, tol, max_iter)\n",
        "raiz_newton, it_newton = newton_raphson(x0, tol, max_iter)\n",
        "\n",
        "x = np.linspace(-3, 0, 100)\n",
        "plt.plot(x, f(x), label='f(x) = $(cos(x))^2 - cos(x)$')\n",
        "plt.axhline(0, color='red', linestyle='--')\n",
        "plt.title('Gráfico da função')\n",
        "plt.xlabel('x')\n",
        "plt.ylabel('f(x)')\n",
        "plt.legend()\n",
        "plt.grid()\n",
        "plt.show()\n",
        "\n",
        "print(\"Tabela de iterações - Bisseção:\")\n",
        "print(\"Iteração | a       | b       | c       | f(c)\")\n",
        "for it in it_bissecao:\n",
        "    print(f\"{it[0]:>8} | {it[1]:<7.4f} | {it[2]:<7.4f} | {it[3]:<7.4f} | {it[4]:<7.4f}\")\n",
        "\n",
        "print(\"\\nTabela de iterações - Newton-Raphson:\")\n",
        "print(\"Iteração | x       | f(x)\")\n",
        "for it in it_newton:\n",
        "    print(f\"{it[0]:>8} | {it[1]:<7.4f} | {it[2]:<7.4f}\")\n"
      ]
    }
